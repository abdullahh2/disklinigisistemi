doctype html
html
  head

    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    // link(rel='icon' href='assets/images/favicon-32x32.png' type='image/png')

    // plugins
    link(href='/assets/plugins/vectormap/jquery-jvectormap-2.0.2.css' rel='stylesheet')
    link(href='/assets/plugins/simplebar/css/simplebar.css' rel='stylesheet')
    link(href='/assets/plugins/perfect-scrollbar/css/perfect-scrollbar.css' rel='stylesheet')

    // loader
    link(href='/assets/css/pace.min.css' rel='stylesheet')
    script(src='/assets/js/pace.min.js')
    // Bootstrap CSS
    link(href='/assets/css/bootstrap.min.css' rel='stylesheet')
    link(href='/assets/css/bootstrap-extended.css' rel='stylesheet')
    link(href='https://fonts.googleapis.com/css2?family=Roboto:wght@400;500&display=swap' rel='stylesheet')
    link(href='/assets/css/app.css' rel='stylesheet')
    link(href='/assets/css/icons.css' rel='stylesheet')
    // Theme Style CSS
    link(rel='stylesheet' href='/assets/css/dark-theme.css')
    link(rel='stylesheet' href='/assets/css/semi-dark.css')
    link(rel='stylesheet' href='/assets/css/header-colors.css')

    // Custom CSS
    block csscode
    //title
    title= title

  body
    .wrapper
      .header-wrapper
        include ./static/header
        include ./static/navigation

      .page-wrapper
        .page-content
          block content

    .overlay.toggle-icon
    a(href="javaScript:;").back-to-top
      i.bx.bxs-up-arrow-alt
    include ./static/footer
    include ./partials/switcher
    script(src='/assets/js/bootstrap.bundle.min.js')
    // plugins
    script(src='/assets/js/jquery.min.js')
    script(src='/assets/plugins/simplebar/js/simplebar.min.js')
    script(src='/assets/plugins/perfect-scrollbar/js/perfect-scrollbar.js')
    script(src='/assets/plugins/vectormap/jquery-jvectormap-2.0.2.min.js')
    script(src='/assets/plugins/vectormap/jquery-jvectormap-world-mill-en.js')
    script(src='/assets/plugins/chartjs/js/chart.js')
    script(src='/assets/js/index.js')
    // app JS
    script(src='/assets/js/app.js')

    // Custom JS
    block jscode
